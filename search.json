---
layout: none
---

[
{% for page in site.pages %}
  {% if page.title %}
  {
    "title": "{{ page.title | escape }}",
    "url": "{{ page.url | absolute_url }}",
    "content": "{{ page.content | strip_html | strip_newlines | truncate: 200 }}"
  }{% unless forloop.last %},{% endunless %}
  {% endif %}
{% endfor %}
]
